<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".management.presentation.ManagementFragment">

    <androidx.appcompat.widget.Toolbar
        style="@style/Theme.PruebaRSS"
        android:id="@+id/management_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:menu="@menu/manager_toolbar_menu"
        android:theme="@style/Widget.Material3.Toolbar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/list_rss_management"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="@dimen/bottom_menu_height"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/management_toolbar"
        app:layout_constraintTop_toBottomOf="@id/management_toolbar"
        tools:listitem="@layout/view_item_rss_management"/>

</androidx.constraintlayout.widget.ConstraintLayout>