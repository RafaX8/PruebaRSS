<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".management.presentation.ManagementFragment">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/management_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:menu="@menu/manager_toolbar_menu"
        android:theme="@style/Widget.Material3.Toolbar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sample_management"
        app:layout_constraintStart_toStartOf="@id/management_toolbar"
        app:layout_constraintTop_toBottomOf="@id/management_toolbar"/>

</androidx.constraintlayout.widget.ConstraintLayout>